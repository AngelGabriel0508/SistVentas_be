	<queryString language="SQL">
		<![CDATA[SELECT 
    p.id AS product_id, 
    p.name AS product_name, 
    cp.name AS category_name,  
    p.price_purchase, 
    p.price_sale, 
    p.date_expiry, 
    p.stock
